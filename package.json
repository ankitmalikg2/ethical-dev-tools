{
  "name": "ethical-dev-tools",
  "displayName": "Ethical Dev Tools (Diff Checker, Base64 Encoder & YAML Validator)",
  "description": "Essential developer tools including side-by-side diff viewer, Base64 encoder/decoder, and YAML validator. Boost productivity with file comparison, encoding utilities, and YAML syntax validation.",
  "version": "0.0.4",
  "publisher": "AnkitMalikTools",
  "icon": "media/ethical-dev-tools.png",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "diff",
    "diff checker",
    "compare",
    "file diff",
    "text diff",
    "code comparison",
    "merge tool",
    "base64",
    "encode",
    "decode",
    "yaml",
    "yaml validator",
    "yaml syntax",
    "yaml parser",
    "yaml checker",
    "ethical",
    "developer tools",
    "utilities",
    "file compare",
    "text compare",
    "diff viewer",
    "base64 encoder",
    "base64 decoder",
    "developer utilities",
    "productivity",
    "coding tools",
    "development tools",
    "json tools",
    "xml tools",
    "text manipulation",
    "string utilities",
    "configuration files",
    "devops tools",
    "syntax validation",
    "side by side",
    "version control"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ethical-dev-tools.createDiff",
        "title": "Ethical Dev Tools: Create Diff View"
      },
      {
        "command": "ethical-dev-tools.base64Tool",
        "title": "Ethical Dev Tools: Base64 Tool"
      },
      {
        "command": "ethical-dev-tools.yamlValidator",
        "title": "Ethical Dev Tools: YAML Validator"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ethical-dev-tools-sidebar",
          "title": "ETHICAL DEV",
          "icon": "$(tools)"
        }
      ]
    },
    "views": {
      "ethical-dev-tools-sidebar": [
        {
          "id": "ethical-dev-tools-view",
          "name": "TOOLS"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.91.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2"
  },
  "homepage": "https://github.com/ankitmalikg2/ethical-dev-tools",
  "bugs": {
    "url": "https://github.com/ankitmalikg2/ethical-dev-tools/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ankitmalikg2/ethical-dev-tools.git"
  },
  "dependencies": {
    "@types/js-yaml": "^4.0.9",
    "js-yaml": "^4.1.0"
  }
}
